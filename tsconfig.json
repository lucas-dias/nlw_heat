{
  "compilerOptions": {
    
    "target": "es2017",                         
  
    /* Modules */
    "module": "commonjs",                    
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    /* Type Checking */
    "strict": false,
    "skipLibCheck": true,
    "typeRoots": ["./src/@types", "node_modules/@types"]
  }
}
